{
  "name": "monarchy-website",
  "version": "1.1.0",
  "description": "A new website for Monarchy Media",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon server.js --ignore js/",
    "debug": "node --inspect-brk server.js",
    "start:testProd": "NODE_ENV=production nodemon server.js",
    "prod": "node server.js NODE_ENV=production",
    "dev:js": "webpack --config webpack.dev.js",
    "build:js": "webpack --config webpack.prod.js",
    "dev:sass": "node-sass src/scss/main.scss public/css/app.min.css -w",
    "compile:sass": "node-sass src/scss/main.scss public/css/app.comp.css",
    "prefix:css": "postcss public/css/app.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/app.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "npm-run-all build:js compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": ".git"
  },
  "author": "Mattia Rasulo",
  "license": "ISC",
  "engines": {
    "node": "14.16.0",
    "npm": "7.7.6"
  },
  "dependencies": {
    "aws-sdk": "^2.515.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cookieconsent": "^3.1.1",
    "cors": "^2.8.5",
    "counterup": "^1.0.2",
    "countup.js": "^2.0.4",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-rate-limit": "^5.2.6",
    "express-recaptcha": "^5.0.0",
    "express-sanitizer": "^1.0.5",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "jquery": "^3.5.1",
    "jquery-scrollify": "^1.0.20",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "mongoose-slug-updater": "^3.1.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.14",
    "owl.carousel": "^2.3.4",
    "owl.carousel2": "^2.2.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "request": "^2.88.0",
    "sanitize-filename": "^1.6.2",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.28.0",
    "slugify": "^1.5.0",
    "validator": "^11.1.0",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concat": "^1.0.3",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-cli": "^7.1.1",
    "querystringify": ">=2.0.0",
    "sass-loader": "^11.0.1",
    "sendevent": "^1.0.5",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.7.2",
    "watch": "^1.0.2",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
